<h1>My Entries</h1>

<% @entries.each do |entry| %>
  <hr>
  <%= current_user %>
  <%= entry.updated_at %>
  <%= entry.title %>
  <%= entry.content %>
  <%= link_to 'Show entry', entry_path(entry) %>
  <br>
  <%= link_to 'Delete entry', entry_path(entry), method: :delete, data: { confirm: "Are you sure?" } %>
  <hr>
<% end %>
<%= link_to 'Go back to home page', root_path  %>
